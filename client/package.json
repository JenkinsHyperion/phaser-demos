  "devDependencies": {
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "fork-ts-checker-webpack-plugin": "^1.5.1",
    "nodemon": "^1.19.3",
    "short-uuid": "^3.1.1",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
